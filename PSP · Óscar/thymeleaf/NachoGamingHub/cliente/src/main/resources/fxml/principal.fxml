<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.pantallas.principal.PrincipalController">
    <center>
        <StackPane prefHeight="150.0" prefWidth="200.0">
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <StackPane layoutX="63.0" layoutY="14.0" prefHeight="150.0" prefWidth="200.0" />
            </AnchorPane>
        </StackPane>
    </center>
    <top>
        <MenuBar fx:id="menuPrincipal">
            <Menu fx:id="menuPersonajes" mnemonicParsing="false" text="Personajes">
                <MenuItem fx:id="menuItemListarPersonajes" mnemonicParsing="false" onAction="#menuClick" text="Filtros" />
                <MenuItem fx:id="menuItemLlamadasPersonajes" mnemonicParsing="false" onAction="#menuClick" text="Llamar" />
            </Menu>
            <Menu fx:id="menuTemporada" mnemonicParsing="false" text="Temporadas">
                <MenuItem fx:id="menuItemListarTemporadas" mnemonicParsing="false" onAction="#menuClick" text="Listar" />
            </Menu>
            <Menu fx:id="menuEpisodios" mnemonicParsing="false" text="Episodios">
                <MenuItem fx:id="menuItemListarEpisodios" mnemonicParsing="false" onAction="#menuClick" text="Filtros" />
                <MenuItem fx:id="menuItemLlamadasEpisodios" mnemonicParsing="false" onAction="#menuClick" text="Llamar" />
            </Menu>
        </MenuBar>
    </top>
</BorderPane>
