<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="450" prefWidth="749" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.pantallas.orders.delete.DeleteController">
    <TableView fx:id="orderlist" layoutX="14.0" layoutY="100.0" onMouseClicked="#selectedUser" prefHeight="200.0"
               prefWidth="300.0">
        <columns>
            <TableColumn fx:id="orderid" prefWidth="54.199981689453125" text="order_id"/>
            <TableColumn fx:id="tableid" prefWidth="64.0" text="table_id"/>
            <TableColumn fx:id="customerid" prefWidth="74.79998779296875" text="customer_id"/>
            <TableColumn fx:id="orderdate" prefWidth="109.20002746582031" text="order_date"/>
        </columns>
    </TableView>
    <TableView fx:id="orderitemlist" layoutX="357.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="order_item_id" prefWidth="75.0" text="order_item_id"/>
            <TableColumn fx:id="order_id" prefWidth="75.0" text="order_id"/>
            <TableColumn fx:id="menu_item_id" prefWidth="75.0" text="menu_item_id"/>
            <TableColumn fx:id="quantity" prefWidth="75.0" text="quantity"/>
        </columns>
    </TableView>
    <Button layoutX="300.0" layoutY="332.0" mnemonicParsing="false" onAction="#deleteOrder" text="DELETE"/>
    <Label layoutX="190.0" layoutY="14.0" prefHeight="54.0" prefWidth="220.0" text="DELETE ORDERS">
        <font>
            <Font size="29.0"/>
        </font>
    </Label>
    <Label layoutX="27.0" layoutY="82.0" text="ORDERS"/>
    <Label layoutX="420.0" layoutY="73.0" text="ORDER ITEMS"/>
</AnchorPane>
